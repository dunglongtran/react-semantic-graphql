{
  "name": "react-semantic-graphql",
  "version": "1.0.0",
  "description":
    "GraphCMS Blog using Apollo Client , Semantic UI and create-react-app",
  "repository": "https://github.com/dunglongtran/react-semantic-graphql",
  "author": "Dung Tran <dunglongtran@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.3.5",
    "apollo-link-http": "^1.5.4",
    "graphql": "^14.0.0-rc.2",
    "graphql-tag": "^2.9.2",
    "node-sass": "4.9.2",
    "node-sass-chokidar": "1.3.0",
    "react": "^16.4.1",
    "react-apollo": "^2.1.9",
    "react-dom": "^16.4.1",
    "react-markdown": "^3.3.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "semantic-ui-css": "2.3.2",
    "semantic-ui-react": "0.82.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css":
      "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "standard && react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "standard --fix"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "standard": "^11.0.1"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
